{
  "name": "gulp-example-project",
  "version": "0.0.1-alpha",
  "private": true,
  "description": "Example AngularJS project, structured by feature rather than file-type. Build script using gulp.js.",
  "main": "server/server.js",
  "scripts": {
    "start": "npm install && nodemon --exec \"npm run gulp\" --watch package.json --watch gulpfile.js --watch gulp --ignore .git --ignore .idea --ignore node_modules --ignore client --ignore dist --ignore server --ignore bower.json --ext js,json",
    "gulp": "npm install && gulp",
    "test": "npm install && gulp test",
    "dist": "npm install && gulp dist --production",
    "bower": "npm install && gulp bower",
    "clean-all": "npm run clean-build && npm run clean-deps",
    "clean-build": "npm install && gulp clean-build",
    "clean-deps": "npm run clean-bower && npm run clean-node_modules",
    "clean-bower": "npm install && gulp clean-bower",
    "clean-node_modules": "rm -rf node_modules"
  },
  "productionConfig": {
    "description": "This configuration is for the built app, in production mode. ($(description))",
    "main": "server.js",
    "scripts": {
      "start": "npm install --production && NODE_ENV=production node server.js"
    },
    "devDependencies": {}
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hugojosefson/gulp-example-project.git"
  },
  "keywords": [
    "angularjs",
    "gulp",
    "by-feature",
    "by",
    "feature",
    "project",
    "structure",
    "template",
    "example"
  ],
  "author": "Hugo Josefson <hugo@josefson.org> (http://about.me/hugojosefson)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hugojosefson/gulp-example-project/issues"
  },
  "homepage": "https://github.com/hugojosefson/gulp-example-project",
  "engines": {
    "node": ">=8.1.4"
  },
  "dependencies": {
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "percolator": "^1.5.0"
  },
  "devDependencies": {
    "bower": "^1.8.0",
    "browser-sync": "^2.18.13",
    "chai": "^4.1.1",
    "clone": "^2.1.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-angular-filesort": "^1.1.1",
    "gulp-angular-templatecache": "^2.0.0",
    "gulp-bower": "0.0.13",
    "gulp-concat": "^2.6.1",
    "gulp-develop-server": "^0.5.2",
    "gulp-if": "^2.0.2",
    "gulp-ignore": "^2.0.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-mocha": "^4.3.1",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-production-package-json": "^1.0.2",
    "gulp-sequence": "^0.4.6",
    "gulp-uglify": "^3.0.0",
    "is-production": "^1.0.1",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "merge-stream": "^1.0.1",
    "minimist": "^1.2.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "phantomjs": "^2.1.7"
  }
}
